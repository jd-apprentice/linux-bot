{
  "name": "executor",
  "version": "1.0.0",
  "author": "Jonathan Dyallo",
  "main": "lib/executor",
  "dependencies": {
    "@libsql/kysely-libsql": "^0.3.0",
    "discord.js": "^14.13.0",
    "dotenv": "^16.4.5",
    "kysely": "^0.27.3"
  },
  "description": "linux bot",
  "imports": {
    "#config": "./src/config.js",
    "#types": "./@types/index.d.ts"
  },
  "keywords": [],
  "license": "GPL-3.0-only",
  "scripts": {
    "build": "bun build ./bot.js --compile --outfile lib/executor",
    "build:arm": "bun build ./bot.js --compile --target=bun-linux-arm64 --outfile lib/executor_arm64",
    "start": "bun lib/executor",
    "dev": "bun --watch bot.js",
    "test": "bun test --coverage",
    "migration": "./scripts/migration.sh",
    "docs": "jsdoc ./src/*.js -d ./docs",
    "lint": "eslint ./src/*.js",
    "lint:fix": "eslint ./src/*.js --fix",
    "format": "prettier --write ./src/*.js",
    "format:check": "prettier --check ./src/*.js",
    "prepare": "husky"
  },
  "type": "module",
  "devDependencies": {
    "@eslint/js": "^9.6.0",
    "@types/bun": "^1.1.6",
    "eslint": "9.x",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-sonarjs": "^1.0.3",
    "globals": "^15.6.0",
    "husky": "^9.0.11",
    "jsdoc": "^4.0.3",
    "prettier": "^3.3.2"
  }
}